# {{ ansible_managed }}
{% for net in proxified_by %}
set_real_ip_from net;
{% endfor %}
real_ip_header X-Forwarded-For;

map $http_x_forwarded_proto $myhttps {
        default   '';
        https    on;
}

map $http_x_forwarded_proto$scheme $myscheme {
	default		http;
	httpshttp	https;
	httpshttps	https;
	https		https;
}
